:this.ballY-this.ballRadius,w:this.ballX+this.ballRadius,h:this.ballY+this.ballRadius});};Basketball.prototype.drawBall=function(keanu,data){data=data||{};data.styles=data.styles||{};var x=data.x||0,y=data.y||0,r=data.r||0,isTrail=data.isTrail||false,f=data.styles.fillStyle||"#E08428",w=data.styles.lineWidth||0,s=data.styles.strokeStyle||"#D07317";keanu.ctx.fillStyle=f;keanu.ctx.strokeStyle=s;keanu.ctx.lineWidth=w;keanu.ctx.beginPath();keanu.ctx.arc(x,y,r,0,Math.PI*2,true);keanu.ctx.closePath();keanu.ctx.fill();keanu.ctx.stroke();};Basketball.prototype.version="0.0.0.0.0.0.1-theta-confirmed";Keanu.modules.Basketball=Basketball;}());espn.gamecast.court={};(function($){var COURT=espn.gamecast.court,log=espn.gamecast.debug,$court=null,$courtCanvas=null,$playerPopup=$("#shot-detail-popup"),$shots=$("#shot-wrapper"),popupXBuffer=10,popupYBuffer=15,courtWidth=0,courtHeight=0,plotCourtWidth=94,plotCourtHeight=50,keanu=null,markerRadius=14,hoopRadius=0,home=[],away=[],bounce=0,shotsOnCourt=5,homeFree=0,awayFree=0,cachedOpts=null,shotsViewable=[],transformX=function(x,side,isFreeThrow,isAnim){var displayPadding=isAnim?0:markerRadius;if(isFreeThrow){if(side=="home"){return homeFree-displayPadding;}else{return awayFree-displayPadding;}}else{var newX=side=="home"?x-4:x+4;if(newX<4){newX=4;}if(newX>90){newX=90;}if(newX>=0){return ~~((newX/plotCourtWidth)*courtWidth)-displayPadding;}else{return ~~((Math.abs(newX)/plotCourtWidth)*courtWidth)+newX-displayPadding;}}},transformY=function(y,side,isFreeThrow,isAnim){var displayPadding=isAnim?0:markerRadius;if(isFreeThrow){return courtHeight/2-displayPadding;}else{if(side=="home"){return ~~((y/plotCourtHeight)*courtHeight)-displayPadding;}else{return ~~(((plotCourtHeight-y)/plotCourtHeight)*courtHeight)-displayPadding;}}};COURT.useTrails=true;COURT.init=function(opts){keanu=new Keanu(opts.canvas||"court-render");cachedOpts=opts;$court=$("#court");$courtCanvas=$court.find("#"+(opts.canvas||"court-render"));courtWidth=$courtCanvas.width()