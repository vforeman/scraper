){if((acceptableShots===0&&espn.gamecast.master.shots[i].isFreethrow)||!espn.gamecast.master.shots[i].isFreethrow){recentShots.push(MODEL.getShot(espn.gamecast.master.shots[i]));acceptableShots++;}i++;}else{break;}}return recentShots.reverse();};MODEL.getLastShot=function(){if(espn.gamecast.master.shots.length>0){return MODEL.getShot(espn.gamecast.master.shots[0]);}else{return undefined;}};MODEL.getShotsByPeriod=function(period,opt){period=period||0;var made=false,miss=false;if(opt){made=opt.made||false;miss=opt.miss||false;}var shots=[],foundShots=false,i,len=espn.gamecast.master.shots.length;if(period==0){for(i=0;i<len;i+=1){if(!espn.gamecast.master.shots[i].isFreethrow){shots.push(MODEL.getShot(espn.gamecast.master.shots[i]));}}}else{if(period>midGame){for(i=0;i<len;i+=1){if(espn.gamecast.master.shots[i].period==period){foundShots=true;if(!espn.gamecast.master.shots[i].isFreethrow){if(made&&espn.gamecast.master.shots[i].isScore){shots.push(MODEL.getShot(espn.gamecast.master.shots[i]));}else{if(miss&&!espn.gamecast.master.shots[i].isScore){shots.push(MODEL.getShot(espn.gamecast.master.shots[i]));}else{shots.push(MODEL.getShot(espn.gamecast.master.shots[i]));}}}}else{if(foundShots){break;}}}}else{for(i=len-1;i>=0;i-=1){if(espn.gamecast.master.shots[i].period==period){foundShots=true;if(!espn.gamecast.master.shots[i].isFreethrow){if(made&&espn.gamecast.master.shots[i].isScore){shots.push(MODEL.getShot(espn.gamecast.master.shots[i]));}else{if(miss&&!espn.gamecast.master.shots[i].isScore){shots.push(MODEL.getShot(espn.gamecast.master.shots[i]));}else{shots.push(MODEL.getShot(espn.gamecast.master.shots[i]));}}}}else{if(foundShots){break;}}}shots.reverse();}}return shots;};MODEL.getMadeShotsByPeriod=function(period){return MODEL.getShotsByPeriod(period,{made:true});};MODEL.getMissedShotsByPeriod=function(period){return MODEL.getShotsByPeriod(period,{miss:true});};MODEL.getShotsByPlayer=function(data){if(!data){return;}var id=data.id||0,period=data.period||0,side=data.side||"",periodShots=[],shots=[],i,len;periodShots=MODEL.getShotsByPeriod(period);if(id>0){if(side!=""){for(i=0,len=periodShots.length;i<len;i+=1){if(periodShots[i].player.playerId==id&&MODEL.getTeamSide(periodShots[i].teamId)==side){shots.push(periodShots[i]);}}}else{for(i=0,len=periodShots.length;i<len;i+=1){if(periodShots[i].player.playerId==id){shots.push(periodShots[i]);}}}}else{if(id==0){if(side!=""){for(i=0,len=periodShots.length;i<len;i+=1){if(MODEL.getTeamSide(periodShots[i].teamId)==side){shots.push(periodShots[i]);}}}else{for(i=0,len=periodShots.length;i<len;i+=1){shots.push(periodShots[i]);}}}}return shots;};MODEL.getGameFlowData=function(){return{home:espn.gamecast.master.current.home,away:espn.gamecast.master.current.away};};MODEL.getShotChartControlData=function(){var teams={home:espn.gamecast.master.current.home,away:espn.gamecast.master.current.away,period:espn.gamecast.master.current.period};teams.home=$.extend(true,{},teams.home,{players:espn.gamecast.master.stats.player.home});teams.away=$.extend(true,{},teams.away,{players:espn.gamecast.master.stats.player.away});return teams;};MODEL.getGameReset=function(){var teams={};teams.home=$.extend(true,{},espn.gamecast.master.current.home,{possession:espn.gamecast.master.current.possession=="home"});teams.away=$.extend(true,{},espn.gamecast.master.current.away,{possession:espn.gamecast.master.current.possession=="away"});return teams;};MODEL.getStory=function(){var preview=espn.gamecast.master.preview,recap=espn.gamecast.master.recap,storyData={storyType:"",gameId:espn.gamecast.master.current.id,headline:"",byline:"",story:""};if(espn.gamecast.master.hasStory){if(espn.gamecast.master.current.gameState==="pregame"){storyData.storyType="preview";storyData.headline=preview.headline;storyData.byline=preview.byline;storyData.story=preview.story;}else{if(espn.gamecast.master.current.gameState==="final"){storyData.storyType="recap";storyData.headline=recap.headline;storyData.byline=recap.byline;storyData.story=r