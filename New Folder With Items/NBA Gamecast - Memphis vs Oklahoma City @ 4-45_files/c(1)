adius];new Keanu.modules.Basketball(keanu,{origin:home,control:[home[0]-(home[0]-homeBounce[0])/2,Math.min(home[1],homeBounce[1]),hoopRadius*1.5],destination:homeBounce,easing:"easeOut"},500);}else{var yRandom=~~(Math.random()*10)<5?-bounce:bounce,awayBounce=[away[0]+bounce,away[1]+yRandom,hoopRadius];new Keanu.modules.Basketball(keanu,{origin:away,control:[away[0]-(away[0]-awayBounce[0])/2,Math.min(awayBounce[1],away[1]),hoopRadius*1.5],destination:awayBounce,easing:"easeOut"},500);}};COURT.shoot=function(data){if(!data){return;}var origin=[transformX(data.coordX||0,data.side,data.isFreethrow,true),transformY(data.coordY||0,data.side,data.isFreethrow,true),hoopRadius],control=[],destination,callback=data.callback;if(data.side=="home"){control=[origin[0]+(home[0]-origin[0])/2,Math.min(origin[1],home[1]),hoopRadius*2];destination=home;}else{control=[origin[0]-(origin[0]-away[0])/2,Math.min(origin[1],away[1]),hoopRadius*2];destination=away;}if(!data.isScore){callback=function(){COURT.putsUpABrick(data.side);data.callback&&data.callback();};}new Keanu.modules.Basketball(keanu,{origin:origin,control:control,destination:destination,easing:data.easing,callback:callback});};COURT.plot=function(shots){if(shots&&shots.length>0){for(var i=0,l=shots.length;i<l;i++){COURT.showMarker(shots[i]);}}};COURT.showPlayerPopup=function(el,attempt){if(!el){return;}var $el=$(el);if($el.length>0){var html="",firstName=$el.attr("data-firstName"),lastName=$el.attr("data-lastName"),playerId=$el.attr("data-playerId"),teamId=$el.attr("data-teamId"),details=$el.attr("data-details"),status=$el.attr("data-status"),playText=attempt?(firstName+" "+lastName+" shoots"):$el.attr("data-playText"),color=$el.attr("data-color"),pos=$el.position(),popWidth=0,popHeight=0,posX=pos.left-popupXBuffer,posY=0,popOnTop=true,arrowClass="down-arrow",popCss={"background-color":color,display:"block"},arrowCss={"border-bottom":"0","border-top":"10px solid "+color};if(settings.sport=="nba"){html='<img src="http://a2.esp