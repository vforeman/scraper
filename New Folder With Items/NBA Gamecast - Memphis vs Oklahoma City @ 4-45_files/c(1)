"&&pageLayout=="landscape")||(currentTabView=="live"&&pageLayout=="portrait"))){shot=model.getShot(espn.gamecast.master.shots[espn.gamecast.findIndex(espn.gamecast.master.shots,play.playId,"id")]||undefined);var callback;if(shot){callback=new hollerbackShot(shot,function(){isAnimating=false;lastPlayMod.build(espn.gamecast.master.plays);if(moduleBuilt["plays-tab"]){pbp.update(espn.gamecast.master.plays);}espn.gamecast.publish("animationUpdate",[]);});isAnimating=true;court.showPlay(shot,true);court.shoot($.extend(true,{},shot,{callback:callback}));}}else{if(isShot&&isNew&&((currentMainView=="flow"&&pageLayout=="landscape")||(currentTabView=="game-flow"&&pageLayout=="portrait"))){shot=model.getShot(espn.gamecast.master.shots[espn.gamecast.findIndex(espn.gamecast.master.shots,play.playId,"id")]||undefined);if(shot&&shot.isScore){flow.setCurrentStatus(espn.gamecast.master.current.statusId);flow.createGraph(espn.gamecast.master.current.period,espn.gamecast.master.shots);}lastPlayMod.build(espn.gamecast.master.plays);if(moduleBuilt["plays-tab"]){pbp.update(espn.gamecast.master.plays);}}else{if(!isAnimating){lastPlayMod.build(espn.gamecast.master.plays);if(moduleBuilt["plays-tab"]){pbp.update(espn.gamecast.master.plays);}}}}if(adEventTypes.indexOf("|"+play.eventType+"|")>-1){log("Show ad!"+play.eventType);if(isMobile){setTimeout(function(){espn.gamecast.publish("gameBreak",[play.eventType]);},7000);}else{setTimeout(espn.gamecast.video.playAd,7000);}}};TROLL.handleOotUpdate=function(data,flag,queue){espn.gamecast.debug("OOT update");oots.update(data);};TROLL.handleGameUpdate=function(data,flag,queue){setTimeout(function(){if(settings.sport!="nba"){if(hasShots&&data.pbpSourceId<5){TROLL.handleGameDowngrade();}else{if(!hasShots&&data.pbpSourceId>=5){TROLL.handleGameUpgrade();}}}if(isAnimating){var updateGame=function(){matchup.build(espn.gamecast.master.current);gameReset.updateTeams(model.getGameReset());espn.gamecast.unsubscribe("animationUpdate",updateGame);};espn.gamecast.subscribe("animationUpdate",updateGame);}else{matchup.build(espn.gamecast.master.current);gameReset.updateTeams(model.getGameReset());}},1000);if(gameState&&TROLL.gameStatus[data.statusId]&&gameState!=TROLL.gameStatus[data.statusId]){window.location=window.location;}};TROLL.gameStatus={"2":"live","22":"live","1":"pregame","7":"live","8":"final","6":"pregame","5":"pregame","3":"final","4":"final"};TROLL.handleNstUpdate=function(data,flag,queue){espn.gamecast.debug("nst update");if(moduleBuilt.nst){nst.update(data,flag);}};TROLL.handlePhotoUpdate=function(data,flag,queue){espn.gamecast.debug("photo update");if(moduleBuilt.photos){photo.update(data,flag);}photoGallery.update(data,flag);TROLL.updatePhotoNumber();};TROLL.handleTweetUpdate=function(data,flag,queue){espn.gamecast.debug("Tweet update");if(moduleBuilt.twitter){twitter.update(data,flag);}};TROLL.handleCommentaryUpdate=function(data,flag,queue){espn.gamecast.debug("commentary update");commentary.update(data,flag);};TROLL.handleVideoUpdate=function(data,flag,queue){espn.gamecast.debug("video update");TROLL.updateVideoNumber();if(moduleBuilt.videos){video.update(data,flag);}};TROLL.handleTeamStatsUpdate=function(){espn.gamecast.debug("team stat update");if(moduleBuilt["ts-tab"]){teamStats.update(espn.gamecast.master.stats.team);}teamStatScroller.update(espn.gamecast.master.stats.team);};TROLL.handlePlayerStatsUpdate=function(whichTeam,flag,queue){if(whichTeam==="home"){boxscore.updateHomeTeam(espn.gamecast.master.stats.player.home);}else{boxscore.updateAwayTeam(espn.gamecast.master.stats.player.away);}};TROLL.handleLeadersUpdate=function(type){if(moduleBuilt["leaders-wrapper"]){if(espn.gamecast.master.leaders[type]){leaders.update(espn.gamecast.master.leaders[type],type);}}};TROLL.checkCourtSize=function(updateCourt){log("checking court size. prev = "+prevCourtWidth+" vs new = "+$court.width());if(prevCourtWidth!=$court.width()||updateCourt){TROLL.handleCourtResize();var recentShots=model.getRecentShots(5),last