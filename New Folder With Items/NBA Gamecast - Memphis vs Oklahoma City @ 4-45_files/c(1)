);clearEmpty=false;}};PBP.build=function(data){var plays=data.plays,outputHTML="";awayTeam=data.away;homeTeam=data.home;if(plays&&plays.length>0){for(var i=0,n=plays.length;i<n;i++){outputHTML+=PBP.getPlayHTML(plays[i],i);}}else{outputHTML+='<tr id="play-0"><td colspan="3">'+espn.gamecast.translate("Play-by-Play Currently Unavailable")+"</td></tr>";clearEmpty=true;}$("#play-list").html(outputHTML);};PBP.getPlayHTML=function(play,i){var color="transparent",abbrev="";if(play.teamId==awayTeam.id){color=awayTeam.color;abbrev=awayTeam.abbrev.toUpperCase()+" - ";}else{if(play.teamId==homeTeam.id){color=homeTeam.color;abbrev=homeTeam.abbrev.toUpperCase()+" - ";}}return'<tr class="play" id="play-'+play.playId+'"><td style="border-right:1px solid '+color+'" class="time">'+play.clock+'</td><td class="playText">'+abbrev+play.playText+'</td><td class="score">'+play.awayScore+"-"+play.homeScore+"</td></tr>";};})(jQuery);espn.gamecast.leaders={};(function($){var LD=espn.gamecast.leaders;LD.build=function(leaders){if(leaders.NBARating){LD.update(leaders.NBARating,"nbarating");}if(leaders.ESPNRating){LD.update(leaders.ESPNRating,"espnrating");}if(leaders.points){LD.update(leaders.points,"points");}if(leaders.assists){LD.update(leaders.assists,"assists");}if(leaders.rebounds){LD.update(leaders.rebounds,"rebounds");}};LD.update=function(leaders,type){var outputHTML="",$list;if(type.toLowerCase()==="nbarating"||type.toLowerCase()==="espnrating"){$list=$("#overall-leaders").children("ul");outputHTML+='<li><h2 class="leader-header">'+espn.gamecast.translate("Overall")+"</h2></li>";}else{if(type==="points"){$list=$("#points-leaders").children("ul");outputHTML+='<li><h2 class="leader-header">'+espn.gamecast.translate("Points")+"</h2></li>";}else{if(type==="assists"){$list=$("#assists-leaders").children("ul");outputHTML+='<li><h2 class="leader-header">'+espn.gamecast.translate("Assists")+"</h2></li>";}else{if(type==="rebounds"){$list=$("#rebounds-leaders").children("ul");outputHTML+='<li><